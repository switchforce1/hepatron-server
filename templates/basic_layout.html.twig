<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>{% block title %}Welcome!{% endblock %}</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    {% block stylesheets %}
        {#<link rel="stylesheet" href="{{ asset('build/css/app.css') }}">#}
        <!-- Bootstrap 3.3.7 -->
        {#<link rel="stylesheet" href="{{ asset('plugins/bootstrap/dist/css/bootstrap.min.css') }}">#}
        <link rel="stylesheet" href="{{ asset('copy/plugins/bootstrap4/dist/css/bootstrap.min.css') }}">
        <!-- Ionicons -->
        {#<link rel="stylesheet" href="{{ asset('plugins/Ionicons/css/ionicons.min.css') }}">#}
        <link rel="stylesheet" href="{{ asset('copy/plugins/Ionicons/css/ionicons.min.css') }}">
        <!-- Theme style -->
        {#<link rel="stylesheet" href="{{ asset('plugins/extra_layout/css/AdminLTE.min.css') }}">#}
        <link rel="stylesheet" href="{{ asset('copy/extra_layout/css/AdminLTE.min.css') }}">

        <!-- Theme style -->
        <link rel="stylesheet" href="{{ asset('copy/plugins/iCheck/square/blue.css') }}">
        <!-- Google Font -->
        <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
    {% endblock %}

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the pages via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body class="hold-transition login-page">
    {% block content %}

    {% endblock %}

    {% block javascripts %}
        <!-- jQuery 3 -->
        {#<script src="{{ asset('plugins/jquery/dist/jquery.min.js') }}"></script>#}
        <script src="{{ asset('copy/plugins/jquery/dist/jquery.min.js') }}"></script>
        <!-- Bootstrap 3.3.7 -->
        {#<script src="{{ asset('plugins/bootstrap/dist/js/bootstrap.min.js') }}"></script>#}
        <script src="{{ asset('copy/plugins/bootstrap/dist/js/bootstrap.min.js') }}"></script>
        <!-- FastClick -->
        {#<script src="{{ asset('plugins/fastclick/lib/fastclick.js') }}"></script>#}
        <script src="{{ asset('copy/plugins/fastclick/lib/fastclick.js') }}"></script>
        <!-- AdminLTE App -->
        {#<script src="{{ asset('plugins/extra_leyout/js/adminlte.min.js') }}"></script>#}
        <script src="{{ asset('copy/extra_layout/js/adminlte.min.js') }}"></script>
        <!-- AdminLTE for demo purposes -->
        {#<script src="{{ asset('plugins/js/demo.js') }}"></script>#}
        <script src="{{ asset('copy/extra_layout/js/demo.js') }}"></script>
    {% endblock %}
</body>
</html>
